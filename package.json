{
  "name": "dbs-my-aws-cuentas-api",
  "version": "1.0.0",
  "description": "This template demonstrates how to develop and deploy a simple Node Express API service, backed by DynamoDB database, running on AWS Lambda using the traditional Serverless Framework.",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^6.0.12",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "serverless": "^2.66.1",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.2.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "offline": "tsc --build && nodemon server.js",
    "sls:offline": "tsc --build && serverless offline start",
